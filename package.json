{
	"name": "vite-example",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:ts": "eslint \"src/**/*.{ts,tsx}\"",
		"lint:ts:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
		"lint:scss": "npx stylelint \"**/*.scss\"",
		"lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
		"preview": "vite preview",
		"test:unit:vitest": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:ui": "npx loki test",
		"test:ui:ok": "npx loki approve",
		"test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
		"bundle-visualizer": "npx vite-bundle-visualizer",
		"bundle-visualizer:sun": "npx vite-bundle-visualizer -t sunburst",
		"bundle-visualizer:json": "npx vite-bundle-visualizer -t json",
		"bundle-visualizer:source": "npx vite-bundle-visualizer --sourcemap",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build"
	},
	"dependencies": {
		"@tanstack/react-router": "^1.22.0",
		"@tanstack/router-devtools": "^1.22.0",
		"axios": "^1.6.8",
		"i18next": "^23.10.1",
		"i18next-browser-languagedetector": "^7.2.0",
		"i18next-http-backend": "^2.5.0",
		"immer": "^10.0.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^14.1.0",
		"zustand": "^4.5.2"
	},
	"devDependencies": {
		"@jest/environment": "^29.7.0",
		"@linaria/core": "^6.1.0",
		"@linaria/react": "^6.1.0",
		"@linaria/stylelint-config-standard-linaria": "^6.1.0",
		"@storybook/addon-essentials": "^7.6.17",
		"@storybook/addon-interactions": "^7.6.17",
		"@storybook/addon-links": "^7.6.17",
		"@storybook/addon-onboarding": "^1.0.11",
		"@storybook/addon-styling": "^1.3.7",
		"@storybook/blocks": "^7.6.17",
		"@storybook/builder-vite": "^8.0.3",
		"@storybook/react": "^7.6.17",
		"@storybook/react-vite": "^7.6.17",
		"@storybook/test": "^7.6.17",
		"@stylistic/eslint-plugin": "^1.6.3",
		"@swc/core": "^1.4.11",
		"@tanstack/router-vite-plugin": "^1.23.0",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/react": "^14.2.2",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.11.30",
		"@types/react": "^18.2.73",
		"@types/react-dom": "^18.2.22",
		"@types/rollup": "^0.54.0",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react-swc": "^3.6.0",
		"@wyw-in-js/vite": "^0.5.0",
		"eslint": "^8.57.0",
		"eslint-config-standard-with-typescript": "^43.0.1",
		"eslint-plugin-i18next": "^6.0.3",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-storybook": "^0.8.0",
		"jsdom": "^24.0.0",
		"loki": "^0.34.0",
		"postcss-html": "^1.6.0",
		"sass": "^1.72.0",
		"storybook": "^7.6.17",
		"storybook_vitest_addon": "^0.1.5",
		"storybook-addon-swc": "^1.2.0",
		"storybook-css-modules": "^1.0.8",
		"stylelint": "^16.2.1",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-config-standard-scss": "^13.0.0",
		"stylelint-scss": "^6.2.1",
		"swc": "^1.0.11",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.3",
		"typescript-eslint": "^0.0.1-alpha.0",
		"vite": "^5.2.6",
		"vite-bundle-visualizer": "^1.1.0",
		"vite-plugin-remove-console": "^2.2.0",
		"vite-plugin-stylelint": "^5.3.1",
		"vite-plugin-svgr": "^4.2.0",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^1.4.0"
	},
	"loki": {
		"configurations": {
			"chrome.laptop": {
				"target": "chrome.app",
				"width": 1366,
				"height": 768,
				"deviceScaleFactor": 1,
				"mobile": false
			},
			"chrome.iphone7": {
				"target": "chrome.app",
				"preset": "iPhone 7"
			}
		}
	},
	"Resources": {
		"Layer": {
			"Type": "AWS::Serverless::LayerVersion",
			"Properties": {
				"Description": {
					"Fn::Sub": [
						"Stack ${AWS::StackName} Layer ${ResourceName}",
						{
							"ResourceName": "Layer"
						}
					]
				},
				"ContentUri": "src/Layer",
				"RetentionPolicy": "Retain"
			}
		}
	}
}
